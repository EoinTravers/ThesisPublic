@article{Ackerman2012,
  title = {The persistence of the fluency{\textendash}confidence association in problem solving},
  volume = {19},
  issn = {1069-9384, 1531-5320},
  doi = {10.3758/s13423-012-0305-z},
  abstract = {Confidence in answers is known to be sensitive to the fluency with which answers come to mind. One aspect of fluency is response latency. Latency is often a valid cue for accuracy, showing an inverse relationship with both accuracy rates and confidence. The present study examined the independent latency{\textendash}confidence association in problem-solving tasks. The tasks were ecologically valid situations in which latency showed no validity, moderate validity, and high validity as a predictor of accuracy. In Experiment 1, misleading problems, which often elicit initial wrong solutions, were answered in open-ended and multiple-choice test formats. Under the open-ended test format, latency was absolutely not valid in predicting accuracy: Quickly and slowly provided solutions had a similar chance of being correct. Under the multiple-choice test format, latency predicted accuracy better. In Experiment 2, nonmisleading problems were used; here, latency was highly valid in predicting accuracy. A breakdown into correct and incorrect solutions allowed examination of the independent latency{\textendash}confidence relationship when latency necessarily had no validity in predicting accuracy. In all conditions, regardless of latency's validity in predicting accuracy, confidence was persistently sensitive to latency: The participants were more confident in solutions provided quickly than in those that involved lengthy thinking. The study suggests that the reliability of the latency{\textendash}confidence association in problem solving depends on the strength of the inverse relationship between latency and accuracy in the particular task.},
  language = {en},
  number = {6},
  urldate = {2015-04-24},
  journal = {Psychonomic {Bulletin} \& {Review}},
  author = {{Ackerman}, Rakefet and {Zalmanov}, Hagar},
  year = {2012},
  keywords = {Cognitive Psychology,Fluency,High order cognition,Metamemory,Reaction time analysis},
  pages = {1187--1192},
  file = {Ackerman_Zalmanov_2012_The persistence of the fluency–confidence association in problem solving.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/JX46RUZJ/Ackerman_Zalmanov_2012_The persistence of the fluency–confidence association in problem solving.pdf:application/pdf;Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/PIXQ8EHD/s13423-012-0305-z.html:}
}

@article{Koop2011,
  title = {Response dynamics: {A} new window on the decision process},
  volume = {6},
  shorttitle = {Response dynamics},
  abstract = {The history of judgment and decision making is defined by a trend toward increasingly nuanced explanations of the decision making process. Recently, process models have become incredibly sophisticated, yet the tools available to directly test these models have not kept pace. These increasingly complex process models require increasingly complex process data by which they can be adequately tested. We propose a new class of data collection that will facilitate evaluation of sophisticated process models. Tracking mouse paths during a continuous response provides an implicit measure of the growth of preference that produces a choice---rather than the current practice of recording just the button press that indicates that choice itself. Recent research in cognitive science (Spivey \& Dale, 2006) has shown that cognitive processing can be revealed in these dynamic motor responses. Unlike current process methodologies, these response dynamics studies can demonstrate continuous competition between choice options and even online preference reversals. Here, in order to demonstrate the mechanics and utility of the methodology, we present an example response dynamics experiment utilizing a common multi-alternative decision task.},
  number = {8},
  urldate = {2013-11-11},
  journal = {Judgment and {Decision} {Making}},
  author = {{Koop}, Gregory J. and {Johnson}, Joseph G.},
  year = {2011},
  keywords = {Decision Making,methodology,metrics.,Mouse-tracking,process models,response dynamics},
  pages = {750--758},
  file = {RePEc Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/D8Z32ZQB/v6y2011i8p750-758.html:;RePEc PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/HJKK5CZV/Koop and Johnson - 2011 - Response dynamics A new window on the decision pr.pdf:application/pdf;Response dynamics - A new window on the decision process.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/XR2CBH8F/Response dynamics - A new window on the decision process.pdf:application/pdf}
}

@incollection{Handley2015,
  title = {Dual {Processes} and the {Interplay} between {Knowledge} and {Structure}: {A} {New} {Parallel} {Processing} {Model}},
  volume = {62},
  shorttitle = {Chapter {Two}-{Dual} {Processes} and the {Interplay} between {Knowledge} and {Structure}},
  abstract = {How do reasoners resolve a conflict between two competing responses, one cued by beliefs and knowledge, and one based upon the problem's underlying logic and structure? The literature suggests that such conflicts are routinely resolved in favor of a belief-based heuristic (Type 1) response that is generated autonomously and by default. Given sufficient effort, time, and motivation, reasoners can and sometimes do engage in more deliberative (Type 2) processing and intervene on default responses, generating responses based upon the underlying structure of the problem. Such default interventionist accounts of reasoning biases have become increasingly popular in the reasoning and judgment domains. In this chapter we review recent evidence which suggests that reasoners show intuitive sensitivity to logical structure, which can interfere with belief or knowledge judgments. We further show that the tendency to respond on the basis of beliefs can be slow and effortful, can increase with development, and often depends upon working memory. We evaluate the implications of these findings for dual process accounts and conclude that existing dual process frameworks cannot account for the evidence. Finally, we sketch the principles of a new model which assumes that both structure and knowledge make intuitive and deliberative contributions to responses on reasoning and judgment tasks.},
  urldate = {2015-11-04},
  booktitle = {Psychology of {Learning} and {Motivation}},
  publisher = {{Academic Press}},
  author = {{Handley}, Simon J. and {Trippas}, Dries},
  editor = {{Ross}, Brian H.},
  year = {2015},
  pages = {33--58},
  annote = {Only skimmed through, but this is a review of recent default-interventionist, and logical intuition findings, which concludes that

logic can be fast
belief can be slow
heuristics have to develop as well

Notes pilot data (p.24) using an instructional manipulation on more complex logical problems, and fails to find an influence of logic on belief judgments (contra to their prior work)},
  file = {Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/7WTFD236/S0079742114000036.html:;Handley & Trippas PLM in press.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/BGGJNPKR/Handley & Trippas PLM in press.pdf:application/pdf;ScienceDirect Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/UZSNFQ4V/S0079742114000036.html:}
}

