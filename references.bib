  year = {2013},
  keywords = {cognitive processes,informal programming,Problem Solving,reasoning},
  pages = {16766--16771},
  file = {Khemlani et al_2013_Kinematic mental simulations in abduction and deduction.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/I7M8ZGGC/Khemlani et al_2013_Kinematic mental simulations in abduction and deduction.pdf:application/pdf;Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/N7RD6W2M/16766.html:},
