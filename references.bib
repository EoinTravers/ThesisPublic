@article{Buehner2005,
  title = {{Reasoning=working} memory\ensuremath{\not =}attention},
  volume = {33},
  issn = {0160-2896},
  doi = {10.1016/j.intell.2005.01.002},
  abstract = {The purpose of this study was to clarify the relationship between attention, components of working memory, and reasoning. Therefore, twenty working memory tests, two attention tests, and nine intelligence subtests were administered to 135 students. Using structural equation modeling, we were able to replicate a functional model of working memory proposed by Oberauer, Suess, Wilhelm, and Wittmann (2003) {[}Oberauer, K., Suess, H.-M., Wilhelm, O., \&amp; Wittmann, W. W. (2003). The multiple faces of working memory: Storage, processing, supervision, and coordination. Intelligence, 31, 167{\textendash}193]. The study also revealed a weak to moderate relationship between the {\textquotedblleft}selectivity aspect of attention{\textquotedblright} and working memory components as well as the finding that {\textquotedblleft}supervision{\textquotedblright} was only moderately related to {\textquotedblleft}storage in the context of processing{\textquotedblright} and to {\textquotedblleft}coordination{\textquotedblright}. No significant path was found from attention to reasoning. Reasoning could be significantly predicted by {\textquotedblleft}storage in the context of processing{\textquotedblright} and {\textquotedblleft}coordination{\textquotedblright}. All in all, 95\% of reasoning variance could be explained. Controlling for speed variance, the correlation between working memory components and intelligence did not decrease significantly.},
  number = {3},
  urldate = {2014-07-04},
  journal = {Intelligence},
  author = {{Buehner}, Markus and {Krumm}, Stefan and {Pick}, Marion},
  year = {2005},
  keywords = {Attention,crtPaper,Intelligence,reasoning,working memory},
  pages = {251--272},
  file = {ScienceDirect Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/H4WUNZDJ/S0160289605000164.html:;Buehner et al_2005_Reasoning=working memory≠attention.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/WTIJ96CA/Buehner et al_2005_Reasoning=working memory≠attention.pdf:application/pdf}
