  pages = {179--199},
  file = {Childrens use of generics in Inductive Inferences - Gelman (2002).pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/IT37VT9C/Childrens use of generics in Inductive Inferences - Gelman (2002).pdf:application/pdf;Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/P4H3EKAD/S15327647JCD0302_3.html:;Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/VT7KJK9T/Gelman et al. - 2002 - Children's Use of Generics in Inductive Inferences.pdf:application/pdf}
