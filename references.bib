@article{Evans2010b,
  title = {Intuition and {Reasoning}: {A} {Dual}-{Process} {Perspective}},
  volume = {21},
  issn = {1047-840X},
  shorttitle = {Intuition and {Reasoning}},
  doi = {10.1080/1047840X.2010.521057},
  abstract = {A lay definition of intuition holds that it involves immediate apprehension in the absence of reasoning. From a more technical point of view, I argue also that intuition should be seen as the contrastive of reasoning, corresponding roughly to the distinction between Type 1 (intuitive) and Type 2 (reflective) processes in contemporary dual process theories of thinking. From this perspective, we already know a great deal about intuition: It is quick, provides feelings of confidence, can reflect large amounts of information processing, and is most likely to provide accurate judgments when based on relevant experiential learning. Unlike reasoning, intuition is low effort and does not compete for central working memory resources. It provides default responses which may{\textemdash}or often may not{\textemdash}be intervened upon with high effort, reflective reasoning. Intuition has, however, been blamed for a range of cognitive biases in the psychological literatures on reasoning and decision making. The evidence indicates that with novel and abstract problems, not easily linked to previous experience, intervention with effortful reasoning is often required to avoid such biases. Hence, although it seems that intuition dominates reasoning most of the time{\textemdash}both in the laboratory and the real world{\textemdash}it can indeed be a false friend.},
  number = {4},
  urldate = {2015-08-25},
  journal = {Psychological {Inquiry}},
  author = {{Evans}, Jonathan St B. T.},
  year = {2010},
  pages = {313--326},
  file = {Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/G78QH54G/Evans - 2010 - Intuition and Reasoning A Dual-Process Perspectiv.pdf:application/pdf;Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/P8BCFETZ/1047840X.2010.html:}
}

