@article{Boldt2015,
  title = {Shared {Neural} {Markers} of {Decision} {Confidence} and {Error} {Detection}},
  volume = {35},
  issn = {0270-6474, 1529-2401},
  doi = {10.1523/JNEUROSCI.0797-14.2015},
  abstract = {Empirical evidence indicates that people can provide accurate evaluations of their own thoughts and actions by means of both error detection and confidence judgments. This study investigates the foundations of these metacognitive abilities, specifically focusing on the relationship between confidence and error judgments in human perceptual decision making. Electroencephalography studies have identified the error positivity (Pe){\textemdash}an event-related component observed following incorrect choices{\textemdash}as a robust neural index of participants' awareness of their errors in simple decision tasks. Here we assessed whether the Pe also varies in a graded way with participants' subjective ratings of decision confidence, as expressed on a 6-point scale after each trial of a dot count perceptual decision task. We observed clear, graded modulation of the Pe by confidence, with monotonic reduction in Pe amplitude associated with increasing confidence in the preceding choice. This effect was independent of objective accuracy. Multivariate decoding analyses indicated that neural markers of error detection were predictive of varying levels of confidence in correct decisions, including subtle shifts in high-confidence trials. These results suggest that shared mechanisms underlie two forms of metacognitive evaluation that are often treated separately, with consequent implications for current theories of their neurocognitive basis.},
  language = {en},
  number = {8},
  urldate = {2015-10-01},
  journal = {The {Journal} of {Neuroscience}},
  author = {{Boldt}, Annika and {Yeung}, Nick},
  year = {2015},
  keywords = {cognitive control,confidence,consciousness,Decision Making,error monitoring,error positivity,LondonConsciousness,Metacognition},
  pages = {3478--3484},
  file = {Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/9BBW4DA3/3478.html:;Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/T53QPZKI/Boldt and Yeung - 2015 - Shared Neural Markers of Decision Confidence and E.pdf:application/pdf},
