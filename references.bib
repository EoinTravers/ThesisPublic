  booktitle = {Proceedings of the {SIGCHI} {Conference} on {Human} {Factors} in {Computing} {Systems}},
  publisher = {{ACM}},
  author = {{Huang}, Jeff and {White}, Ryen W. and {Dumais}, Susan},
  year = {2011},
  keywords = {clicks,cursor movements,Eye tracking,Human computer interaction,implicit feedback,Mouse-tracking,web search},
