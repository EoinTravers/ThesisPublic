}

@article{DeNeys2009a,
  title = {Belief inhibition during thinking: {Not} always winning but at least taking part},
  volume = {113},
  issn = {0010-0277},
  shorttitle = {Belief inhibition during thinking},
  doi = {10.1016/j.cognition.2009.07.009},
  abstract = {Human thinking is often biased by intuitive beliefs. Inhibition of these tempting beliefs is considered a key component of human thinking, but the process is poorly understood. In the present study we clarify the nature of an inhibition failure and the resulting belief bias by probing the accessibility of cued beliefs after people reasoned. Results indicated that even the poorest reasoners showed an impaired memory access to words that were associated with cued beliefs after solving reasoning problems in which the beliefs conflicted with normative considerations (Experiment 1 and 2). The study further established that the impairment was only temporary in nature (Experiment 3) and did not occur when people were explicitly instructed to give mere intuitive judgments (Experiment 4). Findings present solid evidence for the postulation of an inhibition process and imply that belief bias does not result from a failure to recognize the need to inhibit inappropriate beliefs, but from a failure to complete the inhibition process. This indicates that people are far more logical than hitherto believed.},
  number = {1},
  urldate = {2015-08-25},
  journal = {Cognition},
  author = {{De Neys}, Wim and {Franssens}, Samuel},
  year = {2009},
  keywords = {BaseRateChapter,Decision-making,inhibition,reasoning},
  pages = {45--61},
  file = {ScienceDirect Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/9TX3NJ45/De Neys and Franssens - 2009 - Belief inhibition during thinking Not always winn.pdf:application/pdf;ScienceDirect Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/AU5ZHF74/S0010027709001681.html:}
}

@article{Bartholow2010,
