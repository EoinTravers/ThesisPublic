@article{Premack2010,
  title = {Why {Humans} {Are} {Unique} {Three} {Theories}},
  volume = {5},
  issn = {1745-6916, 1745-6924},
  doi = {10.1177/1745691609356782},
  abstract = {Three recent papers reject Darwin's claim that there is no fundamental difference between humans and animals. Each offers a unique theory of the difference. The first theory holds that although animals can perceive perceptual relations, humans alone can reinterpret the higher order relations between these relations. The theory offers analogical reasoning as an example of the uniquely human ability to deal with higher order relations between relations. However, chimpanzees are capable of analogical reasoning if the analogies are conceptually simple. The second theory proposes that human intelligence has far better developed social than physical competence{\textemdash}a claim that ignores, and is contradicted by 20 years of infant research showing that the infant's social and physical modules are almost equally developed. The third theory finds that whereas animal abilities are limited adaptations restricted to a single goal, human abilities are domain general and serve indeterminately many goals. This article rejects the first two theories and explains the unique character of domain-general human competence in terms of the interweaving of evolutionarily independent abilities{\textemdash}an interweaving found in humans only.},
  language = {en},
  number = {1},
  urldate = {2015-05-27},
  journal = {Perspectives on {Psychological} {Science}},
  author = {{Premack}, David},
  year = {2010},
  keywords = {analogies,domain-general vs. adaptation,human vs. animal cognition,interweaving of evolutionary independent abilities,modalities,social vs. physical},
  pages = {22--32},
  file = {Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/AN8QW8G3/22.html:}
}

