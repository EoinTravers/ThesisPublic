@article{Jefferies-Sewell2014,
  title = {To admit or not to admit? {The} effect of framing on risk assessment decision making in psychiatrists},
  volume = {24},
  issn = {0963-8237},
  shorttitle = {To admit or not to admit?},
  doi = {10.3109/09638237.2014.951477},
  abstract = {Abstract Background: The way that information is presented is well known to induce a range of biases in human decision tasks. Little research exists on framing effects in psychiatric decision making, but it is reasonable to assume that psychiatrists are not immune and, if so, there may be implications for the welfare of patients, staff and the general public. Aims: To investigate whether presentation of risk information in different formats (frequency, percentage and semantic) influences inpatient admission decisions by psychiatrists. Methods: Six-hundred seventy-eight general adult psychiatrists read a short clinical vignette presenting a case scenario of a patient presenting for inpatient admission. One of four condition questions followed the vignette, incorporating either numerical or percentage probabilities and the semantic labels {\textquotedblleft}high{\textquotedblright} and {\textquotedblleft}low{\textquotedblright} risk. In each condition, the actual risk was identical, but the way it was presented varied. The decision to admit the patient or not was recorded and compared across conditions. Results: More individuals chose to admit the patient when risk information was presented in numerical form (X2\hspace{0.167em}=\hspace{0.167em}7.43, p\hspace{0.167em}=\hspace{0.167em}0.006) and with the semantic label {\textquotedblleft}high{\textquotedblright} (X2\hspace{0.167em}=\hspace{0.167em}7.27, p\hspace{0.167em}=\hspace{0.167em}0.007). Conclusions: Presentation of risk information may influence decision making in psychiatrists. This has important implications for mental health clinical practice where clinicians are required to interpret probabilistic information within their daily work.},
  number = {1},
  urldate = {2015-05-07},
  journal = {Journal of {Mental} {Health}},
  author = {{Jefferies-Sewell}, Kiri and {Sharma}, Shivani and {Gale}, Tim M. and {Hawley}, Chris J. and {Georgiou}, George J. and {Laws}, Keith R.},
  year = {2014},
  pages = {20--23},
  file = {Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/ENEKDJMQ/09638237.2014.html:}
}

