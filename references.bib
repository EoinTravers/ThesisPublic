@article{Stanovich2008,
  title = {On the relative independence of thinking biases and cognitive ability},
  volume = {94},
  copyright = {(c) 2012 {APA}, all rights reserved},
  issn = {1939-1315(Electronic);0022-3514(Print)},
  doi = {10.1037/0022-3514.94.4.672},
  abstract = {In 7 different studies, the authors observed that a large number of thinking biases are uncorrelated with cognitive ability. These thinking biases include some of the most classic and well-studied biases in the heuristics and biases literature, including the conjunction effect, framing effects, anchoring effects, outcome bias, base-rate neglect, "less is more" effects, affect biases, omission bias, myside bias, sunk-cost effect, and certainty effects that violate the axioms of expected utility theory. In a further experiment, the authors nonetheless showed that cognitive ability does correlate with the tendency to avoid some rational thinking biases, specifically the tendency to display denominator neglect, probability matching rather than maximizing, belief bias, and matching bias on the 4-card selection task. The authors present a framework for predicting when cognitive ability will and will not correlate with a rational thinking tendency.},
  number = {4},
  journal = {Journal of {Personality} and {Social} {Psychology}},
  author = {{Stanovich}, Keith E. and {West}, Richard F.},
  year = {2008},
  keywords = {*Cognitive Ability,*Thinking,Cognitive reflection test,crtPaper,Dual process,Heuristics,Intelligence},
  pages = {672--695},
  annote = {In between subject designs (because within subject designs can induce artificial correlations, according to Kahneman), found that lots of heuristics and biases effects aren't predicted all that well by intelligence.
Propose framework where normative response requires (in stages):- Appropriate mindware- Detection of conflict- If the task requires inhibition/decoupling, only then will cognitive ability predict normative choices (high IQ people can decouple better)},
  file = {JPSP08.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/6QCE6RVU/JPSP08.pdf:application/pdf;On the relative independence of thinking biases and cognitive ability..pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/9I449E8B/On the relative independence of thinking biases and cognitive ability..pdf:application/pdf}
}

@article{Lieberman2000,
  title = {Intuition: {A} social cognitive neuroscience approach},
  volume = {126},
  copyright = {(c) 2012 {APA}, all rights reserved},
  issn = {1939-1455(Electronic);0033-2909(Print)},
  shorttitle = {Intuition},
  doi = {10.1037/0033-2909.126.1.109},
  abstract = {This review proposes that implicit learning processes are the cognitive substrate of social intuition. This hypothesis is supported by (a) the conceptual correspondence between implicit learning and social intuition (nonverbal communication) and (b) a review of relevant neuropsychological (Huntington's and Parkinson's disease), neuroimaging, neurophysiological, and neuroanatomical data. It is concluded that the caudate and putamen, in the basal ganglia, are central components of both intuition and implicit learning, supporting the proposed relationship. Parallel, but distinct, processes of judgment and action are demonstrated at each of the social, cognitive, and neural levels of analysis. Additionally, explicit attempts to learn a sequence can interfere with implicit learning. The possible relevance of the computations of the basal ganglia to emotional appraisal, automatic evaluation, script processing, and decision making are discussed.},
  number = {1},
  journal = {Psychological {Bulletin}},
  author = {{Lieberman}, Matthew D.},
  year = {2000},
  keywords = {*Basal Ganglia,*Cognitive Processes,*Intuition,Learning},
  pages = {109--137}
}

