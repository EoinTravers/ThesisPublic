  author = {{Baayen}, R. Harald and {Milin}, Petar},
  year = {2010},
  keywords = {distributions,Dual process,Identidades masculinas,linear mixed-effects modeling,outliers,patriarcado,Reaction times,temporal dependencies,transformations,violencia intrafamiliar},
  pages = {12--28},
