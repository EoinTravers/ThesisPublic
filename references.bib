@article{Torre2008,
  title = {Unraveling the finding of 1/f \ensuremath{\beta} noise in self-paced and synchronized tapping: a unifying mechanistic model},
  volume = {99},
  issn = {0340-1200, 1432-0770},
  shorttitle = {Unraveling the finding of 1/f \ensuremath{\beta} noise in self-paced and synchronized tapping},
  doi = {10.1007/s00422-008-0247-8},
  abstract = {1/f \ensuremath{\beta} noise has been revealed in both self-paced and synchronized tapping sequences, without being consistently taken into consideration for the modeling of underlying timing mechanisms. In this study we characterize variability, short-range, and long-range correlation properties of asynchronies and inter-tap intervals collected in a synchronization tapping experiment, attesting statistically the presence of 1/f \ensuremath{\beta} noise in asynchronies. We verify that the linear phase correction model of synchronization tapping in its original formulation cannot account for the empirical long-range correlation properties. On the basis of previous accounts of 1/f \ensuremath{\beta} noise in the literature on self-paced tapping, we propose an extension of the original synchronization model by modeling the timekeeping process as a source of 1/f \ensuremath{\beta} fluctuations. Simulations show that this `1/f-AR synchronization model' accounts for the statistical properties of empirical series, including long-range correlations, and provides an unifying mechanistic account of 1/f \ensuremath{\beta} noise in self-paced and synchronization tapping. This account opens the original synchronization framework to further investigations of timing mechanisms with regard to the serial correlation properties in performed time intervals.},
  language = {en},
  number = {2},
  urldate = {2013-11-04},
  journal = {Biological {Cybernetics}},
  author = {{Torre}, Kjerstin and {Deligni{\`e}res}, Didier},
  year = {2008},
  keywords = {bioinformatics,Complexity,Computer Appl. in Life Sciences,Linear phase correction,Long-range correlation,neurobiology,Neurosciences,Self-pacing,Synchronization,Tapping,Timing},
  pages = {159--170},
  file = {Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/PSFWKTB2/10.html:;Unraveling the finding of 1f noise in self-pased and synchronized tapping.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/ZFGM5PN5/Unraveling the finding of 1f noise in self-pased and synchronized tapping.pdf:application/pdf}
