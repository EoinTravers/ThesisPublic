@article{Green2008,
  title = {The {Micro}-{Category} account of analogy},
  volume = {106},
  issn = {0010-0277},
  doi = {10.1016/j.cognition.2007.03.015},
  abstract = {Here, we investigate how activation of mental representations of categories during analogical reasoning influences subsequent cognitive processing. Specifically, we present and test the central predictions of the {\textquotedblleft}Micro-Category{\textquotedblright} account of analogy. This account emphasizes the role of categories in aligning terms for analogical mapping. In a semantic priming paradigm, a four-word analogy task was compared to two other four-word tasks. Stimuli were identical in all tasks; only the instructions given to participants differed. Participants were instructed to identify analogy relations, category relations, or conventionalized semantic relations in the four-word sets. After each four-word set, a single target word appeared and participants named this word aloud. Target words that referred to category relations in the preceding four-word sets were primed as strongly when participants identified analogies as when participants identified categories, suggesting that activation of category concepts plays an important role in analogical thinking. In addition, priming of category-referent words in the analogy and category tasks was significantly greater than priming of these words when participants identified conventionalized semantic relations. Since identical stimuli were used in all conditions, this finding indicates that it is the activation of category relations, distinct from any effect of basic semantic association, that causes analogical reasoning to prime category-referent words. We delineate how the {\textquotedblleft}Micro-Category{\textquotedblright} account of analogy predicts these phenomena and unifies findings from diverse areas of research concerning analogical reasoning.},
  number = {2},
  urldate = {2014-04-04},
  journal = {Cognition},
  author = {{Green}, Adam E. and {Fugelsang}, Jonathan A. and {Kraemer}, David J. M. and {Dunbar}, Kevin N.},
  year = {2008},
  keywords = {Adolescent,Adult,Analogical mapping,Analogical reasoning,Analogy,categorization,Facilitation,Female,Humans,Logic,Male,Mental Processes,Micro-Category,Priming,Semantics},
  pages = {1004--1016},
  file = {ScienceDirect Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/E9VPPKK4/Green et al. - 2008 - The Micro-Category account of analogy.pdf:application/pdf;ScienceDirect Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/VCASM6CH/S0010027707000947.html:}
