@article{Griffiths2015,
  title = {Revealing ontological commitments by magic},
  volume = {136},
  issn = {0010-0277},
  doi = {10.1016/j.cognition.2014.10.019},
  abstract = {Considering the appeal of different magical transformations exposes some systematic asymmetries. For example, it is more interesting to transform a vase into a rose than a rose into a vase. An experiment in which people judged how interesting they found different magic tricks showed that these asymmetries reflect the direction a transformation moves in an ontological hierarchy: transformations in the direction of animacy and intelligence are favored over the opposite. A second and third experiment demonstrated that judgments of the plausibility of machines that perform the same transformations do not show the same asymmetries, but judgments of the interestingness of such machines do. A formal argument relates this sense of interestingness to evidence for an alternative to our current physical theory, with magic tricks being a particularly pure source of such evidence. These results suggest that people's intuitions about magic tricks can reveal the ontological commitments that underlie human cognition.},
  urldate = {2015-04-28},
  journal = {Cognition},
  author = {{Griffiths}, Thomas L.},
  year = {2015},
  keywords = {Coincidences,Hierarchies,Magic,Ontological commitments,Predicability},
  pages = {43--48},
  file = {ScienceDirect Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/S4BHUHC8/S0010027714002212.html:;ScienceDirect Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/S4XWVE7I/Griffiths - 2015 - Revealing ontological commitments by magic.pdf:application/pdf}
