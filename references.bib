  doi = {10.1348/026151004X20108},
  abstract = {Children learn many new categories and make inferences about these categories. Much work has examined how children make inferences on the basis of category knowledge. However, inferences may also affect what is learned about a category. Four experiments examine whether category-based inferences during category learning influence category knowledge and thereby affect later classifications for 5- to 7-year-olds. The children learned to classify pictures of new types of creatures on the basis of a salient feature (colour) and then answered a question that required them to make an inference on the basis of other features. At test, children classified pictures that included only some features (without colour). Experiment 1 showed that the features relevant to the inference during learning led to better classification than did features irrelevant to the inference. Experiment 2 replicated this finding even when the relevant features were physically close to the irrelevant features. Experiments 3 and 4 found this effect even when the classification was learned prior to the inference task and even when no mention was made of the categories during inference learning. Taken together, these results show that making inferences during category learning can influence category knowledge and suggest a need to integrate the work on category learning and category-based inferences.},
  language = {en},
  number = {1},
  urldate = {2015-04-08},
  journal = {British {Journal} of {Developmental} {Psychology}},
  author = {{Ross}, Brian H. and {Gelman}, Susan A. and {Rosengren}, Karl S.},
  year = {2005},
  keywords = {SimilarityPaper},
  pages = {1--24},
  file = {Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/2QITP425/Ross et al. - 2005 - Children's category-based inferences affect classi.pdf:application/pdf;026151004X20108.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/42MSDRA3/026151004X20108.pdf:application/pdf;Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/9VGE23TQ/abstract.html:;026151004X20108.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/HGSWEJ33/026151004X20108.pdf:application/pdf}
}

