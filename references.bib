@article{Ware2014,
  title = {You {Get} {What} {You} {Need}: {An} {Examination} of {Purpose}-{Based} {Inheritance} {Reasoning} in {Undergraduates}, {Preschoolers}, and {Biological} {Experts}},
  volume = {38},
  copyright = {{\textcopyright} 2013 {Cognitive} {Science} {Society}, {Inc}.},
  issn = {1551-6709},
  shorttitle = {You {Get} {What} {You} {Need}},
  doi = {10.1111/cogs.12097},
  abstract = {This set of seven experiments examines reasoning about the inheritance and acquisition of physical properties in preschoolers, undergraduates, and biology experts. Participants (N = 390) received adoption vignettes in which a baby animal was born to one parent but raised by a biologically unrelated parent, and they judged whether the offspring would have the same property as the birth or rearing parent. For each vignette, the animal parents had contrasting values on a physical property dimension (e.g., the birth parent had a short tail; the rearing parent had a long tail). Depending on the condition, the distinct properties had distinct functions ({\textquotedblleft}function-predictive{\textquotedblright}) were associated with distinct habitats ({\textquotedblleft}habitat-predictive{\textquotedblright}), or had no implications ({\textquotedblleft}non-predictive{\textquotedblright}). Undergraduates' bias to view properties as inherited from the birth parent was reduced in the function- and habitat-predictive conditions. This result indicates a purpose-based view of inheritance, whereby animals can acquire properties that serve a purpose in their environment. This stance was not found in experts or preschoolers. We discuss the results in terms of how undergraduates' purpose-based inheritance reasoning develops and relates to larger-scale misconceptions about Darwinian evolutionary processes, and implications for biology education.},
  language = {en},
  number = {2},
  urldate = {2014-04-01},
  journal = {Cognitive {Science}},
  author = {{Ware}, Elizabeth A. and {Gelman}, Susan A.},
  year = {2014},
  keywords = {Categories vs Features,Conceptual development,Evolution,Naïve theories,Science education,Teleology},
  pages = {197--243}
}

@article{Mata2013a,
  title = {Reasoning about others' reasoning},
  volume = {49},
  issn = {0022-1031},
  doi = {10.1016/j.jesp.2013.01.010},
  abstract = {Are people more likely to engage in critical thinking when assessing others' reasoning? And does this reasoning enhancement reflect their belief that others are more likely to be biased than themselves (the bias blind spot, BBS)? In three studies, participants who displayed BBS were better able to detect reasoning biases and performed better in reasoning problems when they were asked to examine responses that were said to come from other people than when those same responses were not attributed to other people. Participants who did not display BBS tended to do the opposite. This pattern was found to result from BBS participants engaging in more deliberate thinking when examining the responses of other people than when those answers are not given a social context (Study 2). Moreover, the better or worse performance that resulted from reasoning about others' reasoning transferred to subsequent unrelated problems (Study 3).},
  number = {3},
  urldate = {2014-01-17},
  journal = {Journal of {Experimental} {Social} {Psychology}},
  author = {{Mata}, Andr{\'e} and {Fiedler}, Klaus and {Ferreira}, M{\'a}rio B. and {Almeida}, Tiago},
  year = {2013},
  keywords = {Bias blind spot,crtPaper,Dual process,Epistemic vigilance,reasoning},
