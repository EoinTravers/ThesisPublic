@article{Johansen2015,
  title = {Salience {Not} {Status}: {How} {Category} {Labels} {Influence} {Feature} {Inference}},
  volume = {39},
  copyright = {Copyright {\textcopyright} 2014 {Cognitive} {Science} {Society}, {Inc}.},
  issn = {1551-6709},
  shorttitle = {Salience {Not} {Status}},
  doi = {10.1111/cogs.12206},
  abstract = {Two main uses of categories are classification and feature inference, and category labels have been widely shown to play a dominant role in feature inference. However, the nature of this influence remains unclear, and we evaluate two contrasting hypotheses formalized as mathematical models: the label special-mechanism hypothesis and the label super-salience hypothesis. The special-mechanism hypothesis is that category labels, unlike other features, trigger inference decision making in reference to the category prototypes. This results in a tendency for prototype-compatible inferences because the labels trigger a special mechanism rather than because of any influences they have on similarity evaluation. The super-salience hypothesis assumes that the large label influence is due to their high salience and corresponding impact on similarity without any need for a special mechanism. Application of the two models to a feature inference task based on a family resemblance category structure yields strong support for the label super-salience hypothesis and in particular does not support the need for a special mechanism based on prototypes.},
  language = {en},
  number = {7},
  urldate = {2015-12-03},
  journal = {Cognitive {Science}},
  author = {{Johansen}, Mark K. and {Savage}, Justin and {Fouquet}, Nathalie and {Shanks}, David R.},
  year = {2015},
  keywords = {Classification,concepts,Decision Making,Feature inference},
  pages = {1594--1621},
  file = {Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/B7A2HT45/abstract\;jsessionid=E30720BAF7BEDA0F4A160C2895790599.html:}
