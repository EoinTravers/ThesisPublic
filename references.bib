@article{Mynatt1993,
  title = {Information relevance, working memory, and the consideration of alternatives},
  volume = {46},
  issn = {0272-4987},
  doi = {10.1080/14640749308401038},
  abstract = {People routinely focus on one hypothesis and avoid consideration of alternative hypotheses on problems requiring decisions between possible states of the world{\textendash}-for example, on the {\textquotedblleft}pseudodiagnosticity{\textquotedblright} task (Doherty, Mynatt, Tweney, \& Schiavo, 1979). In order to account for behaviour on such {\textquotedblleft}inference{\textquotedblright} problems, it is proposed that people can hold in working memory, and operate upon, but one alternative at a time, and that they have a bias to test the hypothesis they think true. In addition to being an ex post facto explanation of data selection in inference tasks, this conceptualization predicts that there are situations in which people will consider alternatives. These are: 1. {\textquotedblleft}action{\textquotedblright} problems, where the alternatives are possible courses of action; 2. {\textquotedblleft}inference{\textquotedblright} problems, in which evidence favours an alternative hypothesis. Experiment 1 tested the first prediction. Subjects were given action or inference problems, each with two alternatives and two items of data relevant to each alternative. They received probabilistic information about the relation between one datum and one alternative and picked one value from among the other three possible pairs of such relations. Two findings emerged; (1) a strong tendency to select information about only one alternative with inferences; and (2) a strong tendency, compared to inferences, to select information about both alternatives with actions. Experiment 2 tested the second prediction. It was predicted that data suggesting that one alternative was incorrect would lead many subjects to consider, and select information about, the other alternative. For actions, it was predicted that this manipulation would have no effect. Again the data were as predicted.},
  number = {4},
  urldate = {2015-07-23},
  journal = {The {Quarterly} {Journal} of {Experimental} {Psychology} {Section} {A}},
  author = {{Mynatt}, Clifford R. and {Doherty}, Michael E. and {Dragan}, William},
  year = {1993},
  pages = {759--778},
  file = {Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/2K724PVH/Mynatt et al. - 1993 - Information relevance, working memory, and the con.pdf:application/pdf;Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/3VH6JE4C/14640749308401038.html:}
}

@article{Gonzalez-Garcia2015,
  title = {Unconscious biases in task choices depend on conscious expectations},
  volume = {37},
  issn = {1053-8100},
  doi = {10.1016/j.concog.2015.08.001},
  abstract = {Recent studies highlight the influence of non-conscious information on task-set selection. However, it has not yet been tested whether this influence depends on conscious settings, as some theoretical models propose. In a series of three experiments, we explored whether non-conscious abstract cues could bias choices between a semantic and a perceptual task. In Experiment 1, we observed a non-conscious influence on task-set selection even when perceptual priming and cue-target compound confounds did not apply. Experiments 2 and 3 showed that, under restrictive conditions of visibility, cues only biased task selection when the conscious task-setting mindset led participants to search for information during the time period of the cue. However, this conscious strategy did not modulate the effect found when a subjective measure of consciousness was used. Altogether, our results show that the configuration of the conscious mindset determines the potential bias of non-conscious information on task-set selection.},
  urldate = {2015-10-07},
  journal = {Consciousness and {Cognition}},
  author = {{Gonz{\'a}lez-Garc{\'\i}a}, Carlos and {Tudela}, P{\'\i}o and {Ruz}, Mar{\'\i}a},
  year = {2015},
  keywords = {cognitive control,consciousness,consciousness,Decision-making,LondonConsciousness,Non-conscious bias,Objective and subjective unawareness,Task-set selection},
  pages = {44--56},
