@article{Friston2015,
  title = {{A} {Duet} for one},
  volume = {36},
  issn = {1053-8100},
  doi = {10.1016/j.concog.2014.12.003},
  abstract = {This paper considers communication in terms of inference about the behaviour of others (and our own behaviour). It is based on the premise that our sensations are largely generated by other agents like ourselves. This means, we are trying to infer how our sensations are caused by others, while they are trying to infer our behaviour: for example, in the dialogue between two speakers. We suggest that the infinite regress induced by modelling another agent {\textendash} who is modelling you {\textendash} can be finessed if you both possess the same model. In other words, the sensations caused by others and oneself are generated by the same process. This leads to a view of communication based upon a narrative that is shared by agents who are exchanging sensory signals. Crucially, this narrative transcends agency {\textendash} and simply involves intermittently attending to and attenuating sensory input. Attending to sensations enables the shared narrative to predict the sensations generated by another (i.e. to listen), while attenuating sensory input enables one to articulate the narrative (i.e. to speak). This produces a reciprocal exchange of sensory signals that, formally, induces a generalised synchrony between internal (neuronal) brain states generating predictions in both agents. We develop the arguments behind this perspective, using an active (Bayesian) inference framework and offer some simulations (of birdsong) as proof of principle.},
  urldate = {2015-10-05},
  journal = {Consciousness and {Cognition}},
  author = {{Friston}, Karl and {Frith}, Christopher},
  year = {2015},
  keywords = {Active inference,Attention,Bayesian,Communication,consciousness,Generalised synchrony,LondonConsciousness,Predictive coding,Sensory attenuation,Theory of mind},
  pages = {390--405},
  file = {ScienceDirect Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/6JAEJHNS/S105381001400230X.html:;ScienceDirect Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/9KGBTCG7/Friston and Frith - 2015 - A Duet for one.pdf:application/pdf;ScienceDirect Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/RVICF76S/Friston and Frith - 2015 - A Duet for one.pdf:application/pdf;ScienceDirect Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/VKK4QAZG/S105381001400230X.html:}
}

