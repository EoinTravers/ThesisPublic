  annote = {Comment: Extended draft of Chapter to appear in "An introduction to the scientific foundations of eye movement research and its applications"},
  file = {arXiv.org Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/SD49P2Z2/1506.html:;arXiv\:1506.07194 PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/XAKA89NB/Boccignone - 2015 - Advanced statistical methods for eye movement anal.pdf:application/pdf},
