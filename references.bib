@article{Servant2014,
  title = {Conflict tasks and the diffusion framework: {Insight} in model constraints based on psychological laws},
  volume = {72},
  issn = {0010-0285},
  shorttitle = {Conflict tasks and the diffusion framework},
  doi = {10.1016/j.cogpsych.2014.03.002},
  abstract = {Formal models of decision-making have traditionally focused on simple, two-choice perceptual decisions. To date, one of the most influential account of this process is Ratcliff's drift diffusion model (DDM). However, the extension of the model to more complex decisions is not straightforward. In particular, conflicting situations, such as the Eriksen, Stroop, or Simon tasks, require control mechanisms that shield the cognitive system against distracting information. We adopted a novel strategy to constrain response time (RT) models by concurrently investigating two well-known empirical laws in conflict tasks, both at experimental and modeling levels. The two laws, predicted by the DDM, describe the relationship between mean RT and (i) target intensity (Pi{\'e}ron's law), (ii) standard deviation of RT (Wagenmakers{\textendash}Brown's law). Pioneering work has shown that Pi{\'e}ron's law holds in the Stroop task, and has highlighted an additive relationship between target intensity and compatibility. We found similar results in both Eriksen and Simon tasks. Compatibility also violated Wagenmakers{\textendash}Brown's law in a very similar and particular fashion in the two tasks, suggesting a common model framework. To investigate the nature of this commonality, predictions of two recent extensions of the DDM that incorporate selective attention mechanisms were simulated and compared to the experimental results. Both models predict Pi{\'e}ron's law and the violation of Wagenmakers{\textendash}Brown's law by compatibility. Fits of the models to the RT distributions and accuracy data allowed us to further reveal their relative strengths and deficiencies. Combining experimental and computational results, this study sets the groundwork for a unified model of decision-making in conflicting environments.},
  urldate = {2015-05-18},
  journal = {Cognitive {Psychology}},
  author = {{Servant}, Mathieu and {Montagnini}, Anna and {Burle}, Bor{\'\i}s},
  year = {2014},
  keywords = {cognitive control,Conflict tasks,Decision-making,diffusion model,Piéron’s law,Wagenmakers–Brown’s law},
  pages = {162--195},
  file = {ScienceDirect Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/A7786KZA/Servant et al. - 2014 - Conflict tasks and the diffusion framework Insigh.pdf:application/pdf;ScienceDirect Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/G66NESZU/S0010028514000267.html:}
}

