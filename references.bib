@article{Grau2014,
  title = {Conscious {Brain}-to-{Brain} {Communication} in {Humans} {Using} {Non}-{Invasive} {Technologies}},
  volume = {9},
  doi = {10.1371/journal.pone.0105225},
  abstract = {Human sensory and motor systems provide the natural means for the exchange of information between individuals, and, hence, the basis for human civilization. The recent development of brain-computer interfaces (BCI) has provided an important element for the creation of brain-to-brain communication systems, and precise brain stimulation techniques are now available for the realization of non-invasive computer-brain interfaces (CBI). These technologies, BCI and CBI, can be combined to realize the vision of non-invasive, computer-mediated brain-to-brain (B2B) communication between subjects (hyperinteraction). Here we demonstrate the conscious transmission of information between human brains through the intact scalp and without intervention of motor or peripheral sensory systems. Pseudo-random binary streams encoding words were transmitted between the minds of emitter and receiver subjects separated by great distances, representing the realization of the first human brain-to-brain interface. In a series of experiments, we established internet-mediated B2B communication by combining a BCI based on voluntary motor imagery-controlled electroencephalographic (EEG) changes with a CBI inducing the conscious perception of phosphenes (light flashes) through neuronavigated, robotized transcranial magnetic stimulation (TMS), with special care taken to block sensory (tactile, visual or auditory) cues. Our results provide a critical proof-of-principle demonstration for the development of conscious B2B communication technologies. More fully developed, related implementations will open new research venues in cognitive, social and clinical neuroscience and the scientific study of consciousness. We envision that hyperinteraction technologies will eventually have a profound impact on the social structure of our civilization and raise important ethical issues.},
  number = {8},
  urldate = {2014-09-04},
  journal = {{PLoS} {ONE}},
  author = {{Grau}, Carles and {Ginhoux}, Romuald and {Riera}, Alejandro and {Nguyen}, Thanh Lam and {Chauvat}, Hubert and {Berg}, Michel and {Amengual}, Juli{\`a} L. and {Pascual-Leone}, Alvaro and {Ruffini}, Giulio},
  year = {2014},
  keywords = {consciousness,Dual process},
  pages = {e105225},
  file = {PLoS Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/48PWX52H/Grau et al. - 2014 - Conscious Brain-to-Brain Communication in Humans U.pdf:application/pdf;PLoS Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/PETHE3EC/infodoi10.1371journal.pone.html:}
}

@article{Tsujii2010,
  title = {The role of inferior frontal cortex in belief-bias reasoning: {An} {rTMS} study},
  volume = {48},
  issn = {0028-3932},
  shorttitle = {The role of inferior frontal cortex in belief-bias reasoning},
  doi = {10.1016/j.neuropsychologia.2010.03.021},
  abstract = {The belief-bias effect in syllogistic reasoning refers to the tendency for subjects to be erroneously biased when logical conclusions are incongruent with belief about the world. This study examined the role of inferior frontal cortex (IFC) in belief-bias reasoning using repetitive transcranial magnetic stimulation (rTMS). We used an off-line rTMS method to disrupt IFC activity transiently. Right IFC stimulation significantly impaired incongruent reasoning performance, enhancing the belief-bias effect. Subjects whose right IFC was impaired by rTMS may not be able to inhibit irrelevant semantic processing in incongruent trials. Although left IFC stimulation impaired congruent reasoning, it paradoxically facilitated incongruent reasoning performance, eliminating the belief-bias effect. Subjects whose left IFC was impaired by rTMS may not suffer from interference by irrelevant semantic processing. This study demonstrates for the first time the roles of left and right IFC in belief-bias reasoning using an rTMS approach.},
  number = {7},
  urldate = {2015-08-25},
  journal = {Neuropsychologia},
  author = {{Tsujii}, Takeo and {Masuda}, Sayako and {Akiyama}, Takekazu and {Watanabe}, Shigeru},
  year = {2010},
  keywords = {BaseRateChapter,Belief-bias effect,Inferior frontal cortex,rTMS,Syllogistic reasoning},
  pages = {2005--2008},
  file = {ScienceDirect Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/MJUFKVRM/Tsujii et al. - 2010 - The role of inferior frontal cortex in belief-bias.pdf:application/pdf;ScienceDirect Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/PPFGEG3I/S0028393210001223.html:}
}

