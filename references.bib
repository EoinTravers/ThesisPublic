  pages = {1--38},
  file = {Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/4GW9V4JH/Piccinini and Scarantino - 2011 - Information processing, computation, and cognition.pdf:application/pdf;Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/RWZEURN2/s10867-010-9195-3.html:}
