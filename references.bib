@incollection{Gilbert1999,
  address = {New {York}},
  title = {What the {Mind}'s {Not}},
  urldate = {2014-12-15},
  booktitle = {Dual process theories in social psychology},
  publisher = {{Guilford}},
  author = {{Gilbert}, Daniel T},
  editor = {{Chaiken}, Shelly and {Trope}, Yaacov},
  year = {1999},
  keywords = {crtPaper,Dual process},
  annote = {A nice taxonomy, much earlier than Evans', and relating to social dual process theories.
Uses analogy of drinks machine that can output Cola or water from the same nozzle, which can be based on...
Selective design

One, or other process is activated, in a given context, for a given task.
Chaiken (1987) Heuristic-Systematic model
Petty \& Cacciopo (1986) Elaborartion likelihood
Tesser (1986) Self evaluation maintainance
Predicts no conflict(?)

Competitive design

Analogous to Parallel-competitive model, but with no real candidates in social psych.
Cites Marcel (1981) on simultaneously active word meanings ("bank")

Consolidative design

Output is merged from both processes
Doesn't offer candidate social psych dual process model, but refers to the modularity hypothesis.
In modern terms, describes what I've been thinking of as competition within systems, or between associative (Type 1) systems.

Freeman's work on face and voice

Corrective design

= Default-Interventionist
Obviously favoured by Gilbert
Sequential operations model (Gilbert, Pelham and Krul, '88); Spinozan model (Gilbert, 91); Fiske and Neuberg's continuum model (90); etc.
Takes a panglossian view of this - process 1 usually good, process 2 as quality control and minor modification

Both processes combine (consolidatively) to produce output when needed, no type 2 only output.
Contrasts with more recent work.

The rest of the chapter is a quite nice reflection on Popperian science, although he never cites Popper. Highlights that progress should be measured not in moving closer to the (non-existent) end goal, but moving further from our ignorant starting point.
"Although a talented detective may be able to rule out one or more architectures, no detective can rule *in* just one {[}...] there are behaviour that a particular architecture cannot possibly perform, and hence behaviours  that can be used to rule out that particular. But there is no behaviour that can be performed by only one architecture..."
("Neural behaviorism is no more interesting than the full-body behaviorism that psychologists abandoned several decades ago.") \ensuremath{<}- Nice aside},
  file = {Gilbert (What the minds not).pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/MDX73D7D/Gilbert (What the minds not).pdf:application/pdf}
}

@article{Barber2003,
  title = {Neural {Representation} of {Probabilistic} {Information}},
  volume = {15},
  issn = {0899-7667},
  doi = {10.1162/08997660360675062},
  abstract = {It has been proposed that populations of neurons process information in terms of probability density functions (PDFs) of analog variables. Such analog variables range, for example, from target luminance and depth on the sensory interface to eye position and joint angles on the motor output side. The requirement that analog variables must be processed leads inevitably to a probabilistic description, while the limited precision and lifetime of the neuronal processing units lead naturally to a population representation of information. We show how a time-dependent probability density\ensuremath{\rho}(x; t) over variable x, residing in a specified function space of dimension D, may be decoded from the neuronal activities in a population as a linear combination of certain decoding functions i(x), with coefficients given by the N firing rates ai(t) (generally with D N). We show how the neuronal encoding process may be described by projecting a set of complementary encoding functions i(x) on the probability density \ensuremath{\rho}(x; t), and passing the result through a rectifying nonlinear activation function. We show how both encoders i (x) and decoders i(x) may be determined by minimizing cost functions that quantify the inaccuracy of the representation. Expressing a given computation in terms of manipulation and transformation of probabilities, we show how this representation leads to a neural circuit that can carry out the required computation within a consistent Bayesian framework, with the synaptic weights being explicitly generated in terms of encoders, decoders, conditional probabilities, and priors.},
  number = {8},
  urldate = {2013-11-06},
  journal = {Neural {Computation}},
  author = {{Barber}, M. J. and {Clark}, J. W. and {Anderson}, C. H.},
  year = {2003},
  pages = {1843--1864},
  file = {Neural Computation Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/3AP3CWF3/08997660360675062.html:;Neural Representation of Probabilistic Information.pdf:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/7S8TQPIN/Neural Representation of Probabilistic Information.pdf:application/pdf}
