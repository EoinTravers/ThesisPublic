@article{Vergauwe2013,
  title = {Chronometric evidence for the dual-process mental model theory of conditional},
  volume = {25},
  issn = {2044-5911},
  doi = {10.1080/20445911.2012.744743},
  abstract = {The fact that adults exhibit a defective truth table when evaluating {\textquotedblleft}If p then q{\textquotedblright} conditional statement and judge \ensuremath{\lnot}p cases as irrelevant for the truth value of the conditional has been considered as one of the main evidence against the mental model theory and in favour of Evans' (2007) suppositional account of conditional. If judgements of irrelevance result from some heuristic process, as the suppositional theory assumes, they should be rapid. By contrast, if they result from a demanding and time consuming fleshing out process, as our mental model theory assumes, {\textquotedblleft}irrelevant{\textquotedblright} responses should be the slowest. In the present study, we analyse the time course of responses in a truth table task as a function of their nature and the interpretation of the conditional adopted by the participants. As our mental model theory predicts, {\textquotedblleft}irrelevant{\textquotedblright} responses are the slowest, and response times are a direct function of the number of models each type of response involves.},
  number = {2},
  urldate = {2014-06-19},
  journal = {Journal of {Cognitive} {Psychology}},
  author = {{Vergauwe}, Evie and {Gauffroy}, Caroline and {Morsanyi}, Kinga and {Dagry}, Isabelle and {Barrouillet}, Pierre},
  year = {2013},
  keywords = {Dual process},
