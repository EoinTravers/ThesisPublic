  file = {Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/2PGSIZ3E/887.html:;Snapshot:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/IQBBPH2D/887.html:;Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/JWQUQAUW/Barttfeld et al. - 2015 - Signature of consciousness in the dynamics of rest.pdf:application/pdf;Full Text PDF:/home/eoin/.zotero/zotero/ml3kqeh6.default/zotero/storage/MHXXXQFK/Barttfeld et al. - 2015 - Signature of consciousness in the dynamics of rest.pdf:application/pdf},
